    <script>

        var JsonString = { "employees":  "employees": [{"firstName": "Pat", "lastName": "Murphy"},{

"firstName": "Mary","lastName": "Burke"},{"firstName": "Tom",

"lastName": "Keane"}]}

        function displayJSON(){



            var obj = JSON.parse(JsonString)

            for (employee of obj.employees){

            console.log(employee.firstName)

            }

        }

        </script>

